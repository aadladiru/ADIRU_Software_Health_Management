package HM_Module
public
with ARINC653;
with SHM_DataType;
with SEI;

device systemHM
features
    systemHM_arlarm_in: in event data  port SHM_DataType::errorData;
    systemHM_action: out event data  port SHM_DataType::actionData;
end systemHM;

device implementation systemHM.impl
